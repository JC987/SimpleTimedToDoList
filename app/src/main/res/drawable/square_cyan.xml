    <item>
        <layer-list>
            <item>
                <shape android:shape="rectangle" >
                    <solid android:color="@color/colorPrimaryCyan"/>
                    <size android:width="60dp" android:height="60dp"/>
                </shape>
            </item>

            <item android:left="30dp">
                <shape android:shape="rectangle">
                    <solid android:color="@color/colorSecondaryCyan"/>
                    <size android:width="30dp" android:height="60dp"/>
                </shape>
            </item>
            <item android:left="30dp" android:top="30dp">
                <shape android:shape="rectangle">
                    <solid android:color="@color/colorAccentCyan"/>
                    <size android:width="30dp" android:height="30dp"/>
                </shape>
            </item>
        </layer-list>
    </item>
